<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="-4dp"
        android:right="-4dp"
        android:bottom="8dp"
        android:top="-4dp">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="2px"
                android:color="@color/line"
                android:dashGap="4px"
                android:dashWidth="6px" />
        </shape>
    </item>

</layer-list>
